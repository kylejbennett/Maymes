<div class="page">
	<div class="form">	
		<h1>LOG IN</h1>

		<%= form_tag '/sessions', method: :post do %>
			<p>
				<%= label_tag :email %><br>
				<%= text_field_tag :email %>
			</p>
			<p>
				<%= label_tag :password %><br>
				<%= password_field_tag :password %>
			</p>
			<p>
				<%= submit_tag "SUBMIT", :class=> "submit" %>
			</p>
		<% end %>
	</div>
</div>
